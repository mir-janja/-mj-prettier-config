{
  "$schema": "http://json.schemastore.org/prettierrc",
  "tabWidth": 4,
  "useTabs": false,
  "semi": true,
  "bracketSameLine": true,
  "bracketSpacing": true,
  "arrowParens": "always",
  "printWidth": 100,
  "singleAttributePerLine": true,
  "singleQuote": true,
  "trailingComma": "all",
  "endOfLine": "lf"
}
